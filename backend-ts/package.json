{
  "name": "ibn-backend-api",
  "version": "0.0.1",
  "description": "Backend API for ICTU Blockchain Network - Business logic and database",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "ts-node -r tsconfig-paths/register src/database/migrate.ts",
    "db:seed": "ts-node -r tsconfig-paths/register src/database/seed.ts",
    "db:rollback": "ts-node -r tsconfig-paths/register src/database/rollback.ts",
    "knex:migrate": "ts-node -r tsconfig-paths/register src/database/knex-migrate.ts",
    "knex:seed": "node -r ts-node/register/transpile-only -r tsconfig-paths/register src/database/knex-seed.ts",
    "knex:rollback": "ts-node -r tsconfig-paths/register src/database/knex-rollback.ts",
    "knex:migrate:make": "knex migrate:make --knexfile knexfile.ts -x ts",
    "knex:seed:make": "knex seed:make --knexfile knexfile.ts -x ts",
    "db:verify": "ts-node -r tsconfig-paths/register src/database/verify-schema.ts",
    "phase1:init": "ts-node -r tsconfig-paths/register src/database/phase1-init.ts",
    "phase1:validate": "ts-node -r tsconfig-paths/register src/database/validate-phase1.ts"
  },
  "dependencies": {
    "@types/knex": "^0.15.2",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "fabric-ca-client": "^2.2.20",
    "fabric-network": "^2.2.20",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "module-alias": "^2.2.3",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@core": "dist/core",
    "@middleware": "dist/middleware",
    "@models": "dist/models",
    "@services": "dist/services",
    "@routes": "dist/routes",
    "@utils": "dist/utils",
    "@database": "dist/database",
    "@schemas": "dist/schemas"
  }
}