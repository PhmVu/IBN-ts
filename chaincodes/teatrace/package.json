{
    "name": "teatrace",
    "version": "0.0.3",
    "description": "Tea Traceability Chaincode for IBN v0.0.3",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "fabric-chaincode-node start",
        "test": "mocha --require ts-node/register src/__tests__/**/*.test.ts",
        "test:watch": "npm test -- --watch",
        "test:coverage": "nyc npm test",
        "lint": "eslint src --ext .ts",
        "prepack": "npm run build"
    },
    "engines": {
        "node": ">=18"
    },
    "dependencies": {
        "fabric-contract-api": "^2.5.0",
        "fabric-shim": "^2.5.0"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/mocha": "^10.0.0",
        "@types/chai": "^4.3.0",
        "@types/sinon": "^17.0.0",
        "@types/sinon-chai": "^3.2.0",
        "typescript": "^5.0.0",
        "mocha": "^10.0.0",
        "chai": "^4.3.0",
        "sinon": "^17.0.0",
        "sinon-chai": "^3.7.0",
        "ts-node": "^10.9.0",
        "nyc": "^15.1.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.0.0"
    },
    "nyc": {
        "extension": [
            ".ts"
        ],
        "exclude": [
            "**/*.test.ts",
            "src/__tests__/**"
        ],
        "reporter": [
            "html",
            "text",
            "lcov"
        ],
        "all": true,
        "check-coverage": true,
        "lines": 80,
        "functions": 80,
        "branches": 75,
        "statements": 80
    }
}